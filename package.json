{
  "name": "sdoc",
  "displayName": "SDOC",
  "description": "Preview SDOC files.",
  "version": "0.0.11",
  "publisher": "local",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:sdoc",
    "onCommand:sdoc.preview",
    "onCommand:sdoc.previewToSide"
  ],
  "scripts": {
    "package": "npx vsce package --out dist/ --allow-missing-repository"
  },
  "main": "./src/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "sdoc",
        "aliases": [
          "SDOC"
        ],
        "extensions": [
          ".sdoc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sdoc",
        "scopeName": "source.sdoc",
        "path": "./syntaxes/sdoc.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "sdoc.preview",
        "title": "SDOC: Open Preview"
      },
      {
        "command": "sdoc.previewToSide",
        "title": "SDOC: Open Preview to the Side"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.7.1"
  }
}
